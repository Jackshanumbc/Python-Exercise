import random

#x = [1, 3, 4, 5, 6, 7, 10, 11, 14]
#y = [1, 4, 6, 7, 10, 14]

x = [random.randrange(1,100) for _ in range(1, 20)] 
y = [random.randrange(2,100) for _ in range(3,30)]
common_list = list()

for element in x:
    if element in y:
        common_list.append(element)
        
print('x:')
print(x)
print('y:')
print(y)
print('common_list:')
print(common_list)
